@page
@model RegisterConfirmationModel


<div class="simple_teaser">
    <div class="simple_teaser__body">
        <h1>@ViewData["Title"]</h1>
        <p>We send email to @ViewData["email"].</p>
        <p>Please check your email to confirm your account.</p>
        <a id="login-submit" class="btn btn-lg btn-primary-web-count" href=@ViewData["ReturnUrl"]>Login page</a>
    </div>
</div>